# Buat file ini di folder: uploads/.htaccess

<IfModule mod_rewrite.c>
    RewriteEngine Off
</IfModule>

# Allow access to images
<FilesMatch "\.(jpg|jpeg|png|gif)$">
    Order Allow,Deny
    Allow from all
</FilesMatch>

# Prevent access to PHP files
<FilesMatch "\.php$">
    Order Deny,Allow
    Deny from all
</FilesMatch>